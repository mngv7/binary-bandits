<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER_RIGHT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.protrack.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <VBox alignment="CENTER_LEFT" maxWidth="500" spacing="20.0">
        <Label text="Welcome"/>
        <TextField promptText="Username" maxWidth="500"/>
        <PasswordField promptText="Password" maxWidth="500"/>
        <Button text="Login" onAction="#onHelloButtonClick"/>
        <Label fx:id="welcomeText"/>
    </VBox>
</VBox>

