<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.protrack.profile.ProfileWorkOrdersController"
      prefHeight="500" prefWidth="300">

    <!-- Column 4 [3] -->
